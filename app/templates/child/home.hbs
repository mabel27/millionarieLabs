{{!-- /app/templates/child/home.hbs --}} 

{{outlet}}


<br>
<div class="user col-md-12">

  <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
    <!-- Indicators -->
    <ol class="carousel-indicators">
      <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
      <li data-target="#carousel-example-generic" data-slide-to="1"></li>
      <li data-target="#carousel-example-generic" data-slide-to="2"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner" role="listbox">
      <div class="item active">
        <img src="assets/image/dollar170.png" height="128" width="128">
        <div class="carousel-caption">
          <h3>What is your Balance?</h3>
        </div>
      </div>
      <div class="item">
        <img src="assets/image/ascending24.png" height="128" width="128">
        <div class="carousel-caption">
          <h3>What is the Credit Score?</h3>
        </div>
      </div>
    </div>

    <!-- Controls -->
    <a class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev">
      <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next">
      <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
</div>
<div class="register-container container ">
  <div class="row">
    <div class="account col-xs-12 col-md-6">
      <div class="wow bounceIn data-wow-delay='0.2s'"id="h1">
        <h3><span class="kf">Summary Account.</span><span><img  src="assets/image/check53.png" width="50" height="50" ></span></h3>

      </div>
      <br>
      <div class="table-responsive">
        <table class="table table-hover">
          <tr>
            <th>Balance <span> <img src="assets/image/dollar170.png" height="30" width="30"></span>
            </th>
            <th>Credit Score<span> <img src="assets/image/dollar173.png" height="30" width="30"></span>
            </th>
            <th>Available <span> <img src="assets/image/dollar175.png" height="30" width="30"></span>
            </th>
            <th>Action<span> <img src="assets/image/cursor13.png" height="30" width="30"></span>
            </th>
          </tr>

          <tr>
            <td>100$</td>
            <td>20</td>
            <td>80$</td>
            <td><a id="c_btn" class=" btn btn-default btn-sm" href="#" role="button"><span class="glyphicon glyphicon-usd" aria-hidden="true"></span>Withdraw &raquo;</a>
            </td>
          </tr>

        </table>
      </div>
    </div>
  
    <div class="act col-xs-12 col-md-6">
      <div class="wow bounceIn data-wow-delay='0.5s' "id="h1">
        <h3><span class="kf">Activity</span><span><img  src="assets/image/verified19.png" width="50" height="50" ></span></h3>
      </div>
<br>
      <div id="b1" class="col-md-12">
      <div class="col-md-12">
        <br>
        {{#each activity in model }}
       <p>activity.description, activity.reward<span> <a id="c_btn" class=" btn btn-default btn-sm" href="#" role="button"><span class="glyphicon glyphicon-ok" aria-hidden="true"></span>done</a></span></p>
         {{/each}}
        <br> 
         <p>Walk the dog: 2$ <span> <a id="c_btn" class=" btn btn-default btn-sm" href="#" role="button"><span class="glyphicon glyphicon-ok" aria-hidden="true"></span>done</a></span></p>
        <br>
        <p>Wash the car: 2$ <span> <a id="c_btn" class=" btn btn-default btn-sm" href="#" role="button"><span class="glyphicon glyphicon-ok" aria-hidden="true"></span>done</a></span></p>
      </div>

</div>
    </div>
    
   
  </div>
    
  </div>
